{
    "manifest_version": 3,
    "name": "Custom Search",
    "version": "1.0",
    "description": "A custom search bar on every new tab.",
    "permissions": [
      "activeTab",
      "storage",
      "cookies",
      "history"
    ],
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    }
  }
  